<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object" meta_type="CMF Action Category">
    <object name="import_rss" meta_type="CMF Action" i18n:domain="plone">
      <property name="title" i18n:translate="">Importar RSS</property>
      <property name="description" i18n:translate=""></property>
      <property name="url_expr">string:$object_url/@@importar-rss</property>
      <property name="link_target"></property>
      <property name="icon_expr"></property>
      <property
          name="available_expr">python:object.portal_type == 'VindulaClipping'</property>
      <property name="permissions">
        <element value="View"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>